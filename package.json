{
  "name": "varlet-ui-pro",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx simple-git-hooks",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint src/**"
  },
  "dependencies": {
    "@varlet/ui": "^1.24.4",
    "pinia": "^2.0.3",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.1.5",
    "picocolors": "^1.0.0",
    "prettier": "^2.4.1",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^4.4.3",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.7.10",
    "vue-tsc": "^0.30.1"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-staged --concurrent false",
    "commit-msg": "yarn node scripts/verifyCommit.js $1"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --ext .js,.ts,.vue,.jsx,.tsx"
    ]
  }
}
